// main function of Jack, with a Golang flavor
func main() {
    // call string.new(3) and 3 appendChar calls
    str := "abc"
    if 1!=2 {
        str.appendChar('d')
    }
    // appends newline since I dont support special chars yet in the above
    // NOTE: this appends past the length of str atm!
    str.appendChar('\n')

    for j:=0; j<=str.length()-1; j++ {
        print(str.charAt(j))
    }
    return
}
